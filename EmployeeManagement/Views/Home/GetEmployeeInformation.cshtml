@{
}

<table class="table">
    <thead>
        <tr>
            <th>Employee Name</th>
            <th>Employee Address</th>
            <th>Department Name</th>
            <th>Department Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.EmployeeInformation)
        {
            <tr>
                <td>@item.EmployeeName</td>
                <td>@item.EmployeeAddress</td>
                <td>@item.DepartmentName</td>
                <td>@item.DepartmentDescription</td>
            </tr>
        }

    </tbody>
</table>